{% if page.metafields["pagemf"]["date_time"] != blank %}
  <div class="custom-field custom-field__date-time custom-field__type--date-time">
    <h3 class="custom-field--title">Date Time</h3>
    <div class="custom-field--value">
      {{ page.metafields["pagemf"]["date_time"] | date: "%m/%d/%Y %I:%M:%S %p" }}
    </div>
  </div>
{% endif %}